		Escribir "faltan ',faltan,' días para día de muertos"
		aux2 = numdia - may + jun + jul + ago + sep + oct + nov + (dic - 6)
		faltan1 = aux2
		Escribir "faltan ',faltan,' días para navidad"
	Fin Si
	
	Si mes = jun Entonces
		aux = numdia - jun + jul + ago + sep + oct + (nov - 28)
		faltan = aux
		Escribir "faltan ',faltan,' días para día de muertos"
		aux2 = numdia - jun + jul + ago + sep + oct + nov + (dic - 6)
		faltan1 = aux2
		Escribir "faltan ',faltan,' días para navidad"
	Fin Si
	
	Si mes = jul Entonces
		aux = numdia - jul + ago + sep + oct + (nov - 28)
		faltan = aux
		Escribir "faltan ',faltan,' días para día de muertos"
		aux2 = numdia - jul + ago + sep + oct + nov + (dic - 6 )
		faltan1 = aux2
		Escribir "faltan ',faltan,' días para navidad"
	Fin Si
	
	Si mes = ago Entonces
		aux = numdia - ago + sep + oct + (nov - 28)
		faltan = aux
		Escribir "faltan ',faltan,' días para día de muertos"
		aux2 = numdia - ago + sep + oct + nov + (dic - 6)
		faltan1 = aux2
		Escribir "faltan ',faltan,' días para navidad"
	Fin Si
	
	
	Si sep = sep Entonces
		aux = numdia - sep + oct + (nov - 28)
		faltan = aux
		Escribir "faltan ',faltan,' días para día de muertos"
		aux2 = numdia - sep + oct + nov + (dic - 6)
		faltan1 = aux2
		Escribir "faltan ',faltan,' días para navidad"
	Fin Si
	
	Si mes = oct Entonces
		aux = numdia - oct + (nov - 28)
		faltan = aux
		Escribir "faltan ',faltan,' días para día de muertos"
		aux2 = numdia - oct + nov + (dic - 6)
		faltan1 = aux2
		Escribir "faltan ',faltan,' días para navidad"
	Fin Si
	
	Si mes = nov y numdia < 2 Entonces
		aux = numdia -  (nov - 28)
		faltan = aux
		Escribir "faltan ',faltan,' días para día de muertos"
	Sino 
		aux = numdia - nov + dic + ene + feb + mar + abr + may + jun + jul + ago + sep + oct
		faltan = aux
		Escribir "faltan ',faltan,' días para día de muertos"
	Fin Si
	
	Si mes = nov Entonces
		aux2 = numdia - nov + (dic - 6)
		faltan1 = aux2
		Escribir "faltan ',faltan,' días para navidad"
	Fin Si
	
	Si mes = dic Entonces
		aux = numdia - nov + dic + ene + feb + mar + abr + may + jun + jul + ago + sep + oct
		faltan = aux
		Escribir "faltan ',faltan,' días para día de muertos"
	Fin Si
	
	Si mes = dic y numdia < 25 Entonces
		aux2 = numdia - dic
		faltan1 = aux2
		Escribir "faltan ',faltan,' días para navidad"
	Fin Si
	
	Fin Algoritmo 
